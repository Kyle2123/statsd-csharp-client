<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net40;net45;net46</TargetFrameworks>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>        
  </PropertyGroup>
  <PropertyGroup>
    <PackageId>StatsdClient.HighPerformance</PackageId>
    <PackageVersion>1.1</PackageVersion>
    <Title>High Performance StatsD Client</Title>
    <PackageProjectUrl>https://github.com/Kyle2123/statsd-csharp-client</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/Kyle2123/statsd-csharp-client/blob/master/MIT-LICENCE.md</PackageLicenseUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Copyright>Copyright 2017</Copyright>
    <Authors>Kyle West</Authors>
    <Description>
      High Performance StatsD Client.  Supports client-side metric aggregation and multiple metrics per UDP packet.  Completely thread-safe.
    </Description>
    <PackageReleaseNotes>
      Add support for .NET Standard 2.0
    </PackageReleaseNotes>
    <PackageTags>stats statsd metrics aggregation</PackageTags>
  </PropertyGroup>

</Project>
